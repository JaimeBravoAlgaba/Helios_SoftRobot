addpath('../dataset/');
data = readtable('0x41_240510_1.csv');

%% DATA EXTRACTION AND PREPROCESSING
qx = data{:, 1}*pi/180;
qy = data{:, 2}*pi/180;
qz = data{:, 3}*pi/180;

h0 = data{:, 4};
h1 = data{:, 5};

h0 = minmaxNorm(h0);
h1 = minmaxNorm(h1);

qy = minmaxNorm(qy);
qz = minmaxNorm(qz);

theta = pi/6*sqrt(qz.^2+qy.^2);
phi = atan2(qz, qy);
phi=wrapTo2Pi(phi);

function [x_norm,m,M] = minmaxNorm(x)
mm = mean(x);
    m = min(x);
    M = max(x);
    x_norm = (x-mm)/(M-m);
end